{
  "name": "Hide Status Button",
  "description": "Hide WhatsApp status button.",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./src/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./src/content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "./src/popup/index.html"
  },
  "permissions": ["tabs", "https://web.whatsapp.com/"]
}
