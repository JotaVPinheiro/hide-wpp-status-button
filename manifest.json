{
  "name": "Hide Status Icon",
  "description": "Hide WhatsApp status icon.",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./src/background.js"]
  },
  "browser_action": {
    "default_popup": "./src/popup/index.html"
  },
  "permissions": ["tabs", "https://web.whatsapp.com", "*://*/*"]
}
